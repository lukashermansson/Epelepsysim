{
    "extends": "airbnb",
    "rules": {
        "no-undef": 0,
        "linebreak-style": 0,
        "require-jsdoc": 0,
        "no-plusplus": 0,
        "no-use-before-define": 0,
        "prefer-destructuring": "warn",
        "import/no-extraneous-dependencies": 0,
        "no-mixed-operators": 0,
        "no-unused-vars": "warn",
        "class-methods-use-this": 0

    },
    "parserOptions": {
        "ecmaVersion": 6
      }
}